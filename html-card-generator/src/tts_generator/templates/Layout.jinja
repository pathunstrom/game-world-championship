{#def cards #}

<html lang="en">
<head>
  <title>Generated Cards</title>
  <style>
    * {
        margin: 0;
        padding: 0;
    }

    body {
        width: 4096px;
        height: 4096px;
        display: grid;
        grid-template-rows: repeat(7, 1fr);
        grid-template-columns: repeat(10, 1fr);
    }

    .last {
        grid-column: -1;
        grid-row: -1;
    }
  </style>
</head>
<body>
  {%- for card in cards -%}
  <Card :card="card"></Card>
  {%- endfor -%}
  <CardBack class="last"></CardBack>
</body>
</html>